<!DOCTYPE html>
<html>
<head>
  <title>Contacts</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
  <%= current_user.name %>
  <%= button_to("Log out", session_url, :method => :delete)%>
  <% end %>
<%= yield %>

</body>
</html>
